ini = { SOI ~ (pair | comment)* ~ section* ~ EOI }
WHITESPACE = _{ " " | "\t" }

// section
section = { "[" ~ name ~ "]" ~ body }
name = @{ char+ }
char = { ASCII_ALPHANUMERIC | "." | "_" | "/" | " " }
body = { (NEWLINE ~ (pair | comment)?)* }

// key-value pair
pair = { key ~ "=" ~ value ~ (NEWLINE | EOI) }
key = @{ char+ }
value = @{ (char | "=")* }

comment = { indicator ~ value? ~ (NEWLINE | EOI) }
indicator = { "#" | ";" }
