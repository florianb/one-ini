ini = { SOI ~ (section | pair | comment | blank_line)* ~ ws? ~ EOI }
ws = { (" " | "\t")* }

// section
section = { ws ~ "[" ~ name ~ "]" ~ ws ~ newline? }
char = { ASCII_ALPHANUMERIC | "." | "_" | "/" }
newline = { NEWLINE }

// key-value pair
pair = { ws ~ name ~ ws ~ equals ~ ws ~ value ~ ws ~ newline? }
equals = { "=" }
name = @{ char+ }
value = @{ char* }

comment = { ws ~ delimiter ~ value? ~ newline? }
delimiter = { "#" | ";" }
blank_line = { ws ~ newline }
